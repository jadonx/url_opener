{
  "manifest_version": 3,
  "name": "Open URLs",
  "version": "1.0",
  "description": "Open multiple URLs from a textbox",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  }
}
